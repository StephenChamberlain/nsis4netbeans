<!DOCTYPE fontscolors PUBLIC
    "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN"
    "https://netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <fontcolor name="command" default="command" foreColor="blue">
        <font style="bold" />
    </fontcolor>
    <fontcolor name="comment" default="comment"/>
    <fontcolor name="string" default="string" foreColor="ffCE7B00"/>
    <fontcolor name="literal" default="string" foreColor="ffCE7B00" />
    <fontcolor name="whitespace" default="whitespace"/>
    <fontcolor name="character" default="char"/>
    <fontcolor name="errors" default="error"/>
    <fontcolor name="keyword" default="keyword" foreColor="black"/>    
    <fontcolor name="plugin" default="plugin" foreColor="ff007C00"/>    
    <fontcolor name="number" default="number"/>
    <fontcolor name="operator" default="operator"/>    
    <fontcolor name="separator" default="separator"/>    
    <fontcolor name="function" default="function" foreColor="blue">
        <font style="bold" />
    </fontcolor>
    <fontcolor name="section" default="section" foreColor="blue">
        <font style="bold" />
    </fontcolor>
</fontscolors>